        "laravel-clear-cache",
        "laravel-routes-view-list",
        "laravel-http-status-code-list",
        "laravel-pwa",
        "pwa-laravel",
        "laravel",
        "php",
        "generator",
        "cli",
        "artisan",
        "action",
        "pivot",
        "migrations",
        "services",
        "pattern"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bhargav Raviya",
            "email": "rajtechnologies@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "raviyatechnical/laravel-action-generator": "dev-master",
        "raviyatechnical/laravel-repository-generator": "dev-master",
        "raviyatechnical/laravel-service-generator": "dev-master",
        "raviyatechnical/laravel-pivot-table-generator": "dev-master"
